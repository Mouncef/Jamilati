                                                            <img class="img-responsive" alt="car1" src="{{ asset('uploads/Health/Cover/'~article.filename) }}">
                                                        </a>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <h3>{{ article.titre }}</h3>
                                                        <h6>{{ article.date|date('d/m/Y') }}</h6>
                                                        <p>{{ article.content1 }}</p>
                                                        <a href="{{ path('jamilati_healthpage_view', {'id':article.id}) }}" class="btn btn-default healthbutton">دخول</a>
                                                    </div>
                                                </div>
                                            </div>
                                        {% endfor %}
                                    </div>
                                </div>
                            </div>
                        {% else %}
                            <div class="alert alert-warning text-right">
                                <p>لا يوجد أي محتوى</p>
                            </div>
                        {% endif %}
                        {#################--------------------Latests Health-----------------------###################}
                        <hr class="endhealth"/>

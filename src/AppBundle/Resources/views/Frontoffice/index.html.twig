{% extends 'base.html.twig' %}


{% block REF %}
    <meta name="keywords" content="جميلتي, موضة, جمال, مطبخ, صحة, أسرة, أخبار, مشاهير">
    <meta name="description" content="جميلتي هي مجلة عربية لنساء العربيات. تجدون على هذا الموقع جميع المواظيع المتعلقة بالمرأة العربية" />
{% endblock %}

{% block title %} - الرئيسية{{ parent() }}{% endblock %}

{% block body %}
    <div class="container-fluid">
            <div class="row">
                <div class="col-md-3">
                    <div class="well weel" id="leftbar">
                        <h2 class="titrevideo"> جميلتي <i class="entete2">TV</i>{#<span class="tv"><img src="{{ asset('assets/img/tv.png') }}" /></span>#}</h2>
                        
                        <ul class="nav nav-pills nav-stacked listvideo">
                            {#################--------------------Latests Videos-----------------------###################}
                            {% if articlevideo %}
                                {% for article in articlevideo %}
                                    <li class="item-video">
                                        <iframe src="{{ article.url }}" frameborder="0" allowfullscreen></iframe>
                                        <div align="center">
                                            <a class="btn button-video" href="{{ path('jamilati_videopage_view',{'id':article.id}) }}">إقرأ المقال
                                            </a>
                                        </div>
                                    </li>
                                {% endfor %}
                            {% else %}
                                <div class="alert alert-warning text-right">
                                    <p>لا يوجد أي محتوى</p>
                                </div>
                            {% endif %}
                            {#################--------------------Latests Videos-----------------------###################}
                        </ul>
                        <hr class="endvideo" />
                        <br />
                        <br />
                        <h2 class="titreweather">احوال الطقس اليوم</h2>
                        <br />
                        <div class="weather-app">
                            <div class="left-app">
                                <div class="temperature"><span id="temperature">0</span>&deg;</div> 
                                <div class="location"><span id="location">Unknown</span></div>
                            </div>	
                            <div class="right-app">
                                <div class="top-app">
                                    <img id="icon" width="75px" src="{{ asset('assets/img/weather/codes/03d.png') }}" />
                                </div>
                                <div class="bottom-app">
                                    <div class="humidity">
                                        <img src="{{ asset('assets/img/weather/humidity.png') }}" height="16px" />
                                        <span id="humidity">0</span>%
                                    </div>
                                    <div class="wind">
                                        <span id="wind">0</span> mph <span id="direction">N</span>°
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <hr class="endbar" />
                        <br />
                        <br />
                        <h2 class="titresubscribe">النشرة البريدية</h2>
                        <br />
                        <div class="thumbnail well well-small text-center" id="newsletter">
                            <p>تسجلي في نشرة جميلتي الإلكترونية</p>
                            <br />
                            <form action="{{ path('jamilati_homepage_email') }}" method="get" id="formsub">
                                <div class="input-group">
                                    <input type="email" name="email" class="form-control" placeholder="أدخلي بريدك الإلكتروني" aria-describedby="sizing-addon2" required=true />
                                    <span class="input-group-addon" id="sizing-addon2"><i class="fa fa-envelope" aria-hidden="true" ></i></span>
                                </div>
                                <br />
                                <input type="submit" value="إشتركي الآن" class="btn btn-large" />
                            </form>
                        </div>
                        <br />
                        <hr class="endbar" />
                        <br />
                        <br />
                        <h2 class="titreconseil">نصيحة اليوم</h2>
                        <br />
                        {#################--------------------Latests Advice-----------------------###################}
                        {% if articleadvice %}
                            {% for article in articleadvice|sort %}
                                <blockquote class="blockquote-reverse conseils">
                                    <p class="text-center">{{ article.content|nl2br }}</p>
                                    <footer> <cite title="Source Title">{{ article.author }}</cite></footer>
                                </blockquote>
                            {% endfor %}
                        {% else %}
                            <div class="alert alert-warning text-right">
                                <p>لا يوجد أي محتوى</p>
                            </div>
                        {% endif %}
                        {#################--------------------Latests Advice-----------------------###################}
                        <br />
                        <hr class="endbar" />
                        <br />
                        <br />
                        <h2 class="titrenewconcepts">أحدث الإصدارات</h2>
                        <br />
                        <div class="newconceptgroup">
                            {#################--------------------Latests Inventions-----------------------###################}
                            {% if articleinvention %}
                                {% for article in articleinvention %}
                                    <a id="itemConcept">
                                        <div class="concept">
                                            <img src="{{ asset('uploads/Invention/Images/'~article.filename) }}" class="img-responsive" />
                                            <p class="text-center">{{ article.titre }}</p>
                                        </div>
                                    </a>
                                {% endfor %}
                            {% else %}
                                <div class="alert alert-warning text-right">
                                    <p>لا يوجد أي محتوى</p>
                                </div>
                            {% endif %}
                            {#################--------------------Latests Inventions-----------------------###################}
                        </div>
                        <br />
                        <hr class="endbar" />
                        <br />
                        
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="well" id="contentbar">
                        {% block Articles %}
                        <!-- Actualité -->
                        <h2 class="titreactu"> آخر المستجدات </h2>
                        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">      
                            <!-- Indicators -->
                            <ol class="carousel-indicators" id="points">

                                {% if lastmode %}
                                    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                                {% else %}
                                {% endif %}
                                {% if lastbeauty %}
                                    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                                {% else %}
                                {% endif %}
                                {% if lastkitchen %}
                                    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                                {% else %}
                                {% endif %}
                                {% if lasthealth %}
                                    <li data-target="#carousel-example-generic" data-slide-to="3"></li>
                                {% else %}
                                {% endif %}
                                {% if lastfamily %}
                                    <li data-target="#carousel-example-generic" data-slide-to="4"></li>
                                {% else %}
                                {% endif %}
                                {% if lastnews %}
                                    <li data-target="#carousel-example-generic" data-slide-to="5"></li>
                                {% else %}
                                {% endif %}
                                {% if laststar %}
                                    <li data-target="#carousel-example-generic" data-slide-to="6"></li>
                                {% else %}
                                {% endif %}

                            </ol>
                            <!-- Wrapper for slides -->
                            <div class="carousel-inner" role="listbox">
                                {% if lastmode %}
                                    <div class="item active">
                                        <a href="{{ path('jamilati_modepage_view', {'id':lastmode.id}) }}">
                                            <img src="{{ asset('uploads/Mode/Cover/'~lastmode.filename) }}" alt="{{ lastmode.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lastmode.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if lastbeauty %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_beautypage_view', {'id':lastbeauty.id}) }}">
                                            <img src="{{ asset('uploads/Beauty/Cover/'~lastbeauty.filename) }}" alt="{{ lastbeauty.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lastbeauty.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if lastkitchen %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_kitchenpage_view', {'id':lastkitchen.id}) }}">
                                            <img src="{{ asset('uploads/Kitchen/Cover/'~lastkitchen.filename) }}" alt="{{ lastkitchen.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lastkitchen.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if lasthealth %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_healthpage_view', {'id':lasthealth.id}) }}">
                                            <img src="{{ asset('uploads/Health/Cover/'~lasthealth.filename) }}" alt="{{ lasthealth.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lasthealth.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if lastfamily %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_familypage_view', {'id':lastfamily.id}) }}">
                                            <img src="{{ asset('uploads/Family/Cover/'~lastfamily.filename) }}" alt="{{ lastfamily.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lastfamily.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if lastnews %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_newspage_view', {'id':lastnews.id}) }}">
                                            <img src="{{ asset('uploads/News/Cover/'~lastnews.filename) }}" alt="{{ lastnews.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ lastnews.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                                {% if laststar %}
                                    <div class="item">
                                        <a href="{{ path('jamilati_starspage_view', {'id':laststar.id}) }}">
                                            <img src="{{ asset('uploads/Stars/Cover/'~laststar.filename) }}" alt="{{ laststar.filename }}">
                                            <div class="carousel-caption">
                                                <p>{{ laststar.content1 }}</p>
                                            </div>
                                        </a>
                                    </div>
                                {% else %}

                                {% endif %}

                            </div>
                            <!-- Controls -->
                            <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
                                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
                                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                        <hr class="endbar" />
                        <!-- Fin Actualité -->
                        <br />
                        <!-- Mode -->
                        <h2 class="titremode"> موضة </h2>
                        {#################--------------------Latests Mode-----------------------###################}
                        {% if articlemode %}
                            <div id="mode_carousel" class="carousel slide" data-ride="carousel" data-interval="2500" >
                                <!--Warpper for slides-->
                                <div class="carousel-inner">
                                    {% set maxIdMode = lastmode.id %}
                                    {% for article in articlemode %}
                                        <div class="item {% if article.id == maxIdMode %}active{% endif %}">
                                            <div class="container-fluid">
                                                <div class="row">
                                                    <div class="col-xs-6 caption">
                                                        <h2>{{ article.titre }}</h2>
                                                        <p>{{ article.content1|nl2br }}</p>
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <a href="{{ path('jamilati_modepage_view', {'id':article.id}) }}" >
                                                            <img src="{{ asset('uploads/Mode/Cover/'~article.filename) }}" class="img-responsive" align="right" width="200" />
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {% endfor %}
                                </div>
                                <!-- End Carousel Inner -->
                                <div class="controls">
                                    <ul class="nav">
                                        {% set maxIdMode = lastmode.id %}
                                        {% for key, article in articlemode %}
                                                <li data-target="#mode_carousel" data-slide-to="{{ key }}" class="img-responsive {% if article.id == maxIdMode %}active{% else %} {% endif %}">
                                                    <a href="#">
                                                        <img src="{{ asset('uploads/Mode/Cover/'~article.filename) }}"  width="90"  />
                                                        <small>{{ article.titre }}</small>
                                                    </a>
                                                </li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>
                        {% else %}
                            <div class="alert alert-warning text-right">
                                <p>لا يوجد أي محتوى</p>
                            </div>
                        {% endif %}
                        {#################--------------------Latests Mode-----------------------###################}


                        <hr class="endmode" />
                        <!-- Fin Mode -->
                        <br />
                        <!-- Beauty -->
                        <h2 class="titrebeauty"> جمال </h2>
                        {#################--------------------Latests Beauty-----------------------###################}
                        {% if articlebeauty %}
                            <div class="sliderbeauty">
                                <div id="myCarousel" class="carousel slide text-right">
                                    <div class="pull-right">
                                        <ul style="margin:0px;" class="control-box pager">
                                            <li><a class="prevBeauty" href="#myCarousel" data-slide="prev"><i class="glyphicon glyphicon-chevron-left"></i></a></li>
                                            <li><a class="nextBeauty" href="#myCarousel" data-slide="next"><i class="glyphicon glyphicon-chevron-right"></i></a></li>
                                        </ul>
                                    </div>
                                    <div class="carousel-inner">
                                        {% set maxIdBeauty = lastbeauty.id %}
                                        {% for article in articlebeauty %}
                                            <div class="item {% if article.id == maxIdBeauty %}active{% else %} {% endif %}">
                                                <div class="row">
                                                    <div class="col-sm-8">
                                                        <a href="{{ path('jamilati_beautypage_view', {'id':article.id}) }}">
                                                            <img class="img-responsive" alt="car1" src="{{ asset('uploads/Beauty/Cover/'~article.filename) }}">
                                                        </a>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <h3>{{ article.titre }}</h3>
                                                        <h6>{{ article.date|date('d/m/Y') }}</h6>
                                                        <p>{{ article.content1|nl2br }}</p>
                                                        <a href="{{ path('jamilati_beautypage_view', {'id':article.id}) }}" class="btn btn-default beautybutton">دخول</a>
                                                    </div>
                                                </div>
                                            </div>
                                        {% endfor %}

                                    </div>
                                </div>
                            </div>
                        {% else %}
                            <div class="alert alert-warning text-right">
                                <p>لا يوجد أي محتوى</p>
                            </div>
                        {% endif %}
                        {#################--------------------Latests Beauty-----------------------###################}


                        <hr class="endbeauty" />
                        <!-- Fin Beauty -->
                        <br />
                        <!--Kitchen-->
                        <h2 class="titrekitchen"> مطبخ </h2>

                        {#################--------------------Latests Kitchen-----------------------###################}
                        {% if articlekitchen %}

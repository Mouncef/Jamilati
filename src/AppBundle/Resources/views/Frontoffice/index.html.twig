                        <hr class="endnews"/>
                        <!--Fin News-->
                        <br />
                        <!--Stars-->
                        <h2 class="titrestars"> مشاهير </h2>
                         <!--Testimonials starting-->

                        {#################--------------------Latests Stars-----------------------###################}
                        {% if articlestar %}
                            <div id="testimonials">
                                <div class="row test">
                                    <div class="col-xs-12">
                                        <div class="testimonials-list">
                                            {% for article in articlestar %}
                                                <!-- Single Testimonial -->
                                                <div class="single-testimonial">
                                                    <div class="testimonial-holder">
                                                        <div class="testimonial-content">
                                                            {{ article.content1[:25]|nl2br }}
                                                            <div class="testimonial-caret"><i class="fa fa-caret-down"></i></div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="testimonial-user clearfix">
                                                                <div class="testimonial-user-image"><img src="{{ asset('uploads/Stars/Cover/'~article.filename) }}" alt="{{ article.filename }}" width="76"></div>
                                                                <div class="testimonial-user-name">
                                                                    <p>
                                                                        {{ article.titre }}
                                                                        <br>
                                                                        <a href="{{ path('jamilati_starspage_view',{'id':article.id}) }}">اكتشف</a>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- End of Single Testimonial -->
                                            {% endfor %}
                                        </div>
                                    </div>
                                </div>
                            </div>
